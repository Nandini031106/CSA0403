#include <stdio.h>

int main() {
    int users, files;
    char name[20];

    printf("Enter number of users: ");
    scanf("%d", &users);

    for(int i = 1; i <= users; i++) {
        printf("Enter number of files for user %d: ", i);
        scanf("%d", &files);
        printf("Files of user %d:\n", i);
        for(int j = 0; j < files; j++) {
            scanf("%s", name);
            printf("%s\n", name);
        }
    }
    return 0;
}
