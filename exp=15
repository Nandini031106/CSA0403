#include <stdio.h>

int main() {
    int start, length, i;

    printf("Enter starting block: ");
    scanf("%d", &start);

    printf("Enter length of file: ");
    scanf("%d", &length);

    printf("File allocated at blocks:\n");
    for(i = start; i < start + length; i++) {
        printf("%d ", i);
    }

    return 0;
}
output:
Enter starting block: 6
Enter length of file: 7
File allocated at blocks:
6 7 8 9 10 11 12 
